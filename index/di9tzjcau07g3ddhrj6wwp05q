this.$().on("focus",n.LAUNCHPAD_SELECTORS.CARD_LARGE,this._handler)},_unbindHandler:function(){this.$().off("focus",n.LAUNCHPAD_SELECTORS.CARD_LARGE,this._handler)},didInsertElement:function(){this._super.apply(this,arguments)
this._enableTabbing()},willDestroyElement:function(){this._super.apply(this,arguments)
this._unbindHandler()}})})
