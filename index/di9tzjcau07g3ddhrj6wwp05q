define("s-image-edit/utils/photo-filters",["exports","ember-stdlib/utils/environment","ember"],function(e,t,n){function a(e){return e<0?0:e>255?255:e}function i(e){for(var t=e.length,n=new Array(t),a=0;a<t;a++)n[a]=new Array(3)
var i=[]
n[0][1]=1
n[0][0]=0
n[0][2]=0
for(var a=1;a<t-1;a++){var s=e[a-1],l=e[a],o=e[a+1]
n[a][0]=(l.x-s.x)/6
n[a][1]=(o.x-s.x)/3
n[a][2]=(o.x-l.x)/6
i[a]=(o.y-l.y)/(o.x-l.x)-(l.y-s.y)/(l.x-s.x)}i[0]=0
i[t-1]=0
n[t-1][1]=1
n[t-1][0]=0
n[t-1][2]=0
for(var a=1;a<t;a++){var r=n[a][0]/n[a-1][1]
n[a][1]-=r*n[a-1][2]
n[a][0]=0
i[a]-=r*i[a-1]}for(var a=t-2;a>=0;a--){var r=n[a][2]/n[a+1][1]
n[a][1]-=r*n[a+1][0]
n[a][2]=0
i[a]-=r*i[a+1]}for(var c=[],a=0;a<t;a++)c.push(i[a]/n[a][1])
return c}function s(e){for(var t=i(e),n=t.length,s=[],l=0;l<n-1;l++)for(var o=e[l],r=e[l+1],c=o.x;c<r.x;c++){var d=(c-o.x)/(r.x-o.x),p=1-d,m=d,u=r.x-o.x,h=Math.round(a(p*o.y+m*r.y+u*u/6*((p*p*p-p)*t[l]+(m*m*m-m)*t[l+1])))
s.push({x:c,y:h})}255===s.length&&s.push(e[e.length-1])
return s}function l(e){if(!e||e.length<2)return null
var t=e.sort((function(e,t){return e.x-t.x})),n=s(t),a=n[0]
if(a.x>0)for(var i=a.x;i>=0;i--)n.push(0,{x:i,y:0})
var l=n[n.length-1]
if(l.x<255)for(var i=l.x+1;i<=255;i++)n.push({x:i,y:255})
var o=[]
n.forEach((function(e){var t=Math.sqrt(Math.pow(e.x-e.y,2))
e.x>e.y&&(t*=-1)
o.push(t)}))
return o}function o(e){var t=document.createElement("canvas"),n=t.getContext("2d").createImageData(e)
n.data.set(e.data)
return n}function r(e){var t=e.imageData.width,n=e.imageData.height,a=e.cfg,i=e.imageAttrs,s=t/i.width,l=-1*i.left*s,o=-1*i.top*s,r=Math.min(a.containerWidth*s,t)
return{centerPoint:{x:l+r/2,y:o+Math.min(a.containerHeight*s,n)/2},containerR:r/2}}function c(e){var t=R[e]
if(!t)return[]
var n=t.SPLINE_POINTS
return{MASTER:l(n.MASTER),R:l(n.R),G:l(n.G),B:l(n.B)}}function d(e,t,n,a,i,s,l,o,r,c){void 0===i&&(i=0)
void 0===s&&(s=1)
void 0===l&&(l=1)
